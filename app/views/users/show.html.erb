								<option>Umoja</option>
								<option>Upper Hill</option>
								<option>Valley Road</option>
								<option>Westlands</option>
							</optgroup>

							<optgroup label="Coast">
								<option>Bamburi</option>
								<option>CBD</option>
								<option>Diani</option>
								<option>Kilifi</option>
								<option>Lamu</option>
