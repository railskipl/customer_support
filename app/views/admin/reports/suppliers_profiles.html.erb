<h2>Supplier Profiles</h2>
 <%if params[:subaction] == "supplier_profiles" %>
  <strong>Displaying data for duration <%= params[:report2][:start_date]%> to <%= params[:report2][:end_date]%>
<%else%>
  <strong>Displaying data for 12 Months
<%end%>
<table>
  <thead>
      <tr>
        <th>Supplier</th>
        <th>Supplier Pin Number</th>
        <th>Industry</th>
        <th>Address line 1</th>
        <th>Address line 2</th>
        <th>Town</th>
        <th>City</th>
        <th>Country</th>
        <th>Contact Person 1</th>
        <th>Authorised Responder</th>
        <th>Title</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Telephone Number</th>
        <th>Mobile Number</th>
        <th>Contact Person 2</th>
        <th>Authorised Responder</th>
        <th>Title</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Telephone Number</th>
        <th>Mobile Number</th>
        <th>Contact Person 3</th>
        <th>Authorised Responder</th>
        <th>Title</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Telephone Number</th>
        <th>Mobile Number</th>
        <th>Subscription</th>
        <th>Start Date</th>
        <th>End Date</th>
        <th>Notes</th>
      </tr>
  </thead>
    
  <tbody>
    <%@suppliers.each do |supplier|%>
      <tr>
        <td><%= supplier.supplier_name %></td>
        <td><%= supplier.supplier_vat_Number%></td>
        <td><%= supplier.industry%></td>
        <td><%= supplier.address_line1%></td>
        <td><%= supplier.address_line2%></td>
        <td><%= supplier.town%></td>
        <td><%= supplier.city%></td>
        <td><%= supplier.country%></td>
        <td></td>
        <td><%= supplier.authorised_responder%></td>
        <td><%= supplier.title%></td>
        <td><%= supplier.first_name%></td>
        <td><%= supplier.last_name%></td>
        <td><%= supplier.email%></td>
        <td><%= supplier.telephone_number%></td>
        <td><%= supplier.mobile_number%></td>
        <td></td>
        <td><%= supplier.auth_resp2%></td>
        <td><%= supplier.title2%></td>
        <td><%= supplier.fname2%></td>
        <td><%= supplier.lname2%></td>
        <td><%= supplier.email2%></td>
        <td><%= supplier.tno2%></td>
        <td><%= supplier.mno2%></td>
        <td></td>
        <td><%= supplier.auth_resp3%></td>
        <td><%= supplier.title3%></td>
        <td><%= supplier.fname3%></td>
        <td><%= supplier.lname3%></td>
        <td><%= supplier.email3%></td>
        <td><%= supplier.tno3%></td>
        <td><%= supplier.mno3%></td>
        <td><%= supplier.subscription%></td>
        <td><%= supplier.start_date%></td>
        <td><%= supplier.end_date%></td>
        <td><%= supplier.notes%></td>
      </tr>
      <%end%>
  </tbody>
</table>
<br/><br/>
