  <div class="field">
    <%= f.label :meta_keyword %>
    <%= f.text_area :meta_keyword, :requried => true, :style=>"height: 227px;width: 722px;" %>
  </div>
	
  <div class="actions">
    <%= f.submit class: "btn btn-primary"%>
  </div>
<% end %>
