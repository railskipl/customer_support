<h2>Total number of Registered Companies</h2>
<%if params[:subaction] == "registered_suppliers" %>
  <strong>Displaying data for duration <%= params[:report3][:start_date]%> to <%= params[:report3][:end_date]%>
<%else%>
  <strong>Displaying data for 12 Months
<%end%>
<table>
  <thead>
    <tr>
      <th>Sr. No. </th>
      <th width="10%">Supplier Company</th>
      <th width="10%">Industry</th>
      <th width="10%">Subscription</th>
      <th width="10%">Start Date</th>
      <th width="10%">End Date</th>
    </tr>
  </thead>

  <tbody>
    <% @supplier_registered.each_with_index.each do |supplier,i| %>
      <tr>
        <td><%= i+1%></td>
        <td><%= supplier.supplier_name %></td>
        <td><%= supplier.industry %></td>
        <td><%= supplier.subscription %></td>
        <td><%= supplier.start_date.strftime("%d-%m-%Y")%></td>
        <td><%= supplier.end_date.strftime("%d-%m-%Y")%></td>
        
      </tr>
    <% end %>
  </tbody>
</table>
<br/><br/>