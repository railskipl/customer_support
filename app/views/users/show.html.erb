								<option>Voi</option>
								<option>Watamu</option>
							</optgroup>
							<optgroup label="Kisumu">
							</select>
				    </div>
				  </div>
				</div>
			</div>
			</span> <span class="span3">
			<div class="rgh">
				<div class="image"><%=image_tag @user.avatar.url(:thumb) if @user.avatar.present? %>
				</div>
			</div>
		</span>
<%end%>
